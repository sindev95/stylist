{
    "name": "sindev95/stylist",
    "description": "Laravel 6 theming package.",
    "authors": [
        {
            "name": "Kirk Bushell",
            "email": "torm3nt@gmail.com"
        },
        {
            "name": "Mike Dugan",
            "email": "mike@mjdugan.com"
        },
        {
            "name": "Yassine Kraiem",
            "email": "kamisama953@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravelcollective/html": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "FloatingPoint\\Stylist\\": "src/Stylist"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FloatingPoint\\Stylist\\StylistServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "preferred-install": "dist"
    },
    "license": "MIT",
    "version": "1.0"
}
